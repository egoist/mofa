{
  "name": "mofa",
  "version": "0.0.0",
  "description": "My own helper library, like lodash but customized for me ;)",
  "license": "MIT",
  "repository": "egoist/mofa",
  "author": {
    "name": "EGOIST",
    "email": "0x142857@gmail.com",
    "url": "github.com/egoist"
  },
  "main": "./index.js",
  "jsnext:main": "./src/index.js",
  "scripts": {
    "test": "xo src/**/* && ava test/test-*.js",
    "build": "babel src --out-dir ./"
  },
  "keywords": [
    ""
  ],
  "devDependencies": {
    "ava": "latest",
    "babel-cli": "^6.7.5",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "xo": "latest"
  },
  "xo": {
    "semicolon": false,
    "space": 2
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "add-module-exports"
    ]
  }
}
